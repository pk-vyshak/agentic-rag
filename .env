# Azure OpenAI Deployment Info

# This should be the name of the deployment you created in Azure OpenAI
AZURE_GPT_4o_MINI_ID=gpt-4o-mini-deployment-id

# The API version you're using (example: 2024-05-13)
AZURE_GPT_4o_MINI_API_VERSION=2024-05-13

# Your Azure OpenAI key (from Azure Portal)
AZURE_GPT_4o_MINI_API_KEY=your-azure-openai-api-key

# Azure endpoint format: https://<your-resource-name>.openai.azure.com/
AZURE_GPT_4o_MINI_ENDPOINT=https://your-resource-name.openai.azure.com/
